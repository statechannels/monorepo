  "scripts": {
    "precommit": "lint-staged --quiet",
    "build:ci": "run-s clearContracts build:typescript",
    "build:typescript": "npx tsc -b",
    "build:webpack": "CI=false node scripts/build.js",
    "build": "run-s clearContracts build:typescript build:webpack",
    "clearContracts": "rm -rf ./build/contracts",
    "contracts:compile": "npx etherlime compile --buildDirectory=./build/contracts",
    "contracts:deploy:test": "npx etherlime deploy --compile=false --file=./deployment/deploy-test.js",
    "contracts:deploy": "npx etherlime deploy --compile=false",
    "deployContracts": "NODE_ENV=development npx deploy-contracts",
    "lint:check": "yarn run tslint --config tslint.json --project .",
    "lint:write": "yarn run tslint --config tslint.json --project . --fix",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "prepare": "yarn build:typescript",
    "prettier:check": "npx prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:write": "npx prettier --write 'src/**/*.{ts,tsx}'",
    "start": "npm run clearContracts && node scripts/start.js",
    "startGanache": "node bin/start-ganache.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test:app": "npx run-jest -c ./config/jest/jest.config.js",
    "test:ci:app": "yarn test:app --all --ci --runInBand",
    "test:ci:contracts": "yarn contracts:compile && yarn test:contracts --all --ci --runInBand --bail",
    "test:ci": "yarn run test:ci:contracts && yarn run test:ci:app",
    "test:contracts": "yarn contracts:deploy:test && npx jest -c ./config/jest/jest.contracts.config.js",
    "test": "yarn run test:app --all"
  },
