  "scripts": {
    "build:ci": "yarn build:typescript",
    "build:typescript": "npx tsc -b",
    "build:webpack": "CI=false node scripts/build.js",
    "build": "run-s build:typescript build:webpack",
    "lint:check": "eslint \"src/**/*.{js,ts,tsx}\" --cache",
    "lint:write": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
    "patch-package": "cd ../.. && yarn patch-package",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "prepare": "yarn patch-package",
    "start": "node scripts/start.js",
    "start:integration": "node scripts/start.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "test:app": "npx jest -c ./config/jest/jest.config.js",
    "test:ci:app": "yarn test:app --all --ci --runInBand",
    "test:ci:contracts": "yarn test:contracts --all --ci",
    "test:ci": "yarn test:ci:contracts && yarn test:ci:app",
    "test:contracts": "npx jest --runInBand -c ./config/jest/jest.contracts.config.js --detectOpenHandles",
    "test": "run-s 'test:app --all' 'test:contracts --all'",
    "test:puppeteer": "npx jest --runInBand -c ./config/jest/jest.puppeteer.config.js",
    "puppeteer:script": "npx ts-node -O '{\"module\":\"commonjs\"}' ./puppeteer/scripts/funding.ts ",
    "test:ci:integration": "wait-on http-get://localhost:3055/ http-get://localhost:3000/ && yarn test:puppeteer",
    "precommit": "lint-staged --quiet",
    "build:netlify": "NODE_ENV=production yarn build:webpack",
    "release:netlify": "netlify deploy --site $WALLET_NETLIFY_ID --dir=build"
  },
