{
  "name": "@statechannels/embedded-wallet",
  "license": "MIT",
  "version": "0.0.1",
  "dependencies": {
    "@statechannels/channel-provider": "*",
    "debug": "4.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.1.0",
    "react-scripts": "3.1.2"
  },
  "scripts": {
    "precommit": "lint-staged --quiet",
    "start": "cp ../../node_modules/@statechannels/channel-provider/dist/*.js ./public/ && react-scripts start",
    "build": "react-scripts build && cp -r ../../node_modules/@statechannels/channel-provider/dist/* ./build/",
    "build:typescript": "tsc -b .",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --runInBand",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint:check": "eslint \"*/**/*.{js,ts,tsx}\" --cache",
    "lint:write": "eslint \"*/**/*.{js,ts,tsx}\" --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts*",
      "!./src/**/index.ts*",
      "!./src/**/*.d.ts",
      "!./src/test-utils/**"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/debug": "4.1.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/eslint": "^6.1.3",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.7",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.1",
    "@types/react-router-dom": "5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.8.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "9.4.0",
    "prettier": "1.19.1",
    "typescript": "3.7.2"
  }
}
