{
  "name": "@statechannels/server-wallet",
  "description": "http server statechannel wallet",
  "version": "0.0.1",
  "author": "",
  "dependencies": {
    "@statechannels/client-api-schema": "0.0.3",
    "@statechannels/nitro-protocol": "0.1.6",
    "@statechannels/wallet-core": "0.1.5",
    "ethers": "5.0.7",
    "fp-ts": "2.7.0",
    "knex": "0.20.9",
    "objection": "2.1.2",
    "pg": "7.17.1",
    "pino": "6.2.0"
  },
  "devDependencies": {
    "@pacote/jest-either": "4.0.4",
    "@statechannels/devtools": "0.1.4",
    "@types/body-parser": "^1.19.0",
    "@types/eslint": "6.1.7",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/express-pino-logger": "^4.0.2",
    "@types/jest": "25.1.0",
    "@types/lodash": "4.14.149",
    "@types/nodemon": "1.19.0",
    "@types/pg": "7.14.0",
    "@types/pino": "6.0.0",
    "@types/prettier": "1.19.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "body-parser": "1.19.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-unicorn": "21.0.0",
    "express-pino-logger": "^5.0.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.4",
    "nodemon": "1.19.4",
    "npm-run-all": "4.1.5",
    "pino-pretty": "4.0.0",
    "prettier": "1.19.1",
    "ts-jest": "25.0.0",
    "ts-node": "8.9.1",
    "typescript": "3.7.5"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": "eslint --max-warnings=0"
  },
  "main": "lib/src/wallet/index.js",
  "types": "lib/src/wallet/index.d.ts",
  "private": true,
  "resolutions": {
    "broccoli": "3.3.3",
    "ethereum-transaction-debugger": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz",
    "scrypt": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.0.2.tgz"
  },
  "scripts": {
    "build": "tsc -b .",
    "build:typescript": "tsc -b .",
    "chain:test": "npx jest --runInBand -c ./jest/jest.chain.config.js",
    "db:create": "createdb server_wallet_${NODE_ENV}",
    "db:drop": "dropdb server_wallet_${NODE_ENV}",
    "db:migrate": "npx knex migrate:latest --cwd src/db-admin",
    "db:rollback": "npx knex migrate:rollback --cwd src/db-admin",
    "db:seed": "npx knex seed:run --cwd src/db-admin",
    "db:setup:dev": "yarn db:drop && yarn db:create && yarn db:migrate",
    "lint:check": "eslint \"*/**/*.{js,ts}\" --cache",
    "lint:write": "eslint \"*/**/*.{js,ts}\" --fix",
    "precommit": "lint-staged --quiet",
    "prepare": "yarn build",
    "prestart": "yarn build && npm run db:rollback && npm run db:migrate && npm run db:seed",
    "prettier:check": "npx prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:write": "npx prettier --write 'src/**/*.{ts,tsx}'",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "start:watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/start-dev-server.ts | npx pino-pretty",
    "test": "yarn jest -c ./jest/jest.config.js --runInBand",
    "test:ci": "yarn test --bail",
    "test:e2e": "SERVER_DB_NAME=ping yarn jest -c ./jest/jest.e2e.config.js",
    "test:stress": "NODE_ENV=test SERVER_DB_NAME=ping yarn ts-node ./stress-test/one-ping-one-pong.ts"
  }
}
