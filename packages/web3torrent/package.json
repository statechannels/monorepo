{
  "name": "@statechannels/web3torrent",
  "description": "A paid streaming extension implementation using state channels for the WebTorrent protocol.",
  "version": "0.0.1",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@rimble/connection-banner": "1.2.3",
    "@statechannels/channel-client": "0.0.1",
    "@statechannels/devtools": "0.1.23",
    "bencode": "2.0.1",
    "ethers": "4.0.45",
    "eventemitter3": "4.0.0",
    "firebase": "7.11.0",
    "jszip": "3.2.2",
    "lodash": "4.17.15",
    "node-sass": "4.13.1",
    "pino": "6.2.0",
    "prettier-bytes": "1.0.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-icons": "3.9.0",
    "react-router-dom": "5.1.0",
    "react-scripts": "3.1.2",
    "rxjs": "6.5.4",
    "webtorrent": "0.107.16"
  },
  "devDependencies": {
    "@statechannels/channel-provider": "*",
    "@storybook/addon-actions": "5.3.9",
    "@storybook/addon-knobs": "5.2.5",
    "@storybook/addon-notes": "5.2.5",
    "@storybook/addons": "5.3.9",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.3.9",
    "@types/bencode": "2.0.0",
    "@types/debug": "4.1.5",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/eslint": "6.1.7",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "25.1.0",
    "@types/jszip": "3.1.6",
    "@types/lodash": "4.14.149",
    "@types/node-sass": "4.11.0",
    "@types/pino": "6.0.0",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/supports-color": "5.3.0",
    "@types/webtorrent": "0.107.0",
    "bittorrent-tracker": "9.14.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "ganache-cli": "6.8.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-junit": "9.0.0",
    "lint-staged": "10.0.4",
    "memory-chunk-store": "1.3.0",
    "pino-pretty": "4.0.0",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.6.0",
    "supports-color": "7.0.0",
    "ts-jest": "25.0.0",
    "typescript": "3.7.5",
    "webtorrent-fixtures": "1.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "state-channels",
    "webtorrent",
    "webtorrent extension"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts*": "yarn run eslint:check",
    "src/**/*.{scss,json}": "yarn run prettier:check"
  },
  "main": "src/index.js",
  "resolutions": {
    "webtorrent/end-of-stream": "1.4.1"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "build-and-release:tracker": "./tracker/build-push-release.sh",
    "build:ci": "run-s contracts:compile build",
    "build:typescript": "tsc -b .",
    "contracts:compile": "node ./bin/compile.js",
    "eject": "react-scripts eject",
    "eslint:check": "eslint \"**/*.{js,ts,tsx}\" --cache --max-warnings=0",
    "eslint:write": "eslint \"**/*.{js,ts,tsx}\" --fix --max-warnings=0",
    "lint:check": "run-s eslint:check prettier:check",
    "lint:write": "run-s eslint:write prettier:write",
    "precommit": "lint-staged --quiet",
    "prepare": "yarn contracts:compile",
    "prettier:check": "prettier --check '**/*.{scss,json}' --ignore-path '.eslintignore'",
    "prettier:write": "prettier --write '**/*.{scss,json}' --ignore-path '.eslintignore'",
    "start": "node scripts/start.js",
    "start:alt": "PORT=3333 node scripts/start.js",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "start:tracker": "node tracker/index.js",
    "start:tracker-logs-on": "DEBUG='web3torrent:tracker' yarn start",
    "storybook": "start-storybook -s ./public -p 4568",
    "test": "jest -c ./config/jest/jest.config.js",
    "test:ci": "CI=true yarn test --runInBand --ci --all --forceExit",
    "test:coverage": "yarn test --coverage"
  }
}
