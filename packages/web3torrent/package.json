{
  "name": "@statechannels/web3torrent",
  "version": "0.0.1",
  "description": "A paid streaming extension implementation using state channels for the WebTorrent protocol.",
  "keywords": [
    "webtorrent",
    "state-channels",
    "webtorrent extension"
  ],
  "engines": {
    "node": "10.16.3"
  },
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.ts*",
      "!./src/**/index.ts*",
      "!./src/routes.ts*",
      "!./src/library/testing/*.ts*",
      "!./src/library/time-based-leeching/*.ts*",
      "!./src/utils/copy-to-clipboard.ts",
      "!./src/utils/test-utils.ts",
      "!./src/components/form/form-input/**",
      "!./src/utils/useInterval.ts",
      "!./src/**/react-app-env.d.ts",
      "!./src/**/types.ts",
      "!./src/**/constants.ts",
      "!./test/utils.ts"
    ],
    "globalSetup": "./src/library/testing/setup.js",
    "globalTeardown": "./src/library/testing/teardown.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "main": "src/index.js",
  "dependencies": {
    "bencode": "2.0.1",
    "debug": "4.1.1",
    "eventemitter3": "4.0.0",
    "node-sass": "^4.12.0",
    "prettier-bytes": "^1.0.4",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.1.2",
    "webtorrent": "0.107.16"
  },
  "devDependencies": {
    "@types/bencode": "2.0.0",
    "@types/debug": "4.1.5",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/webtorrent": "0.107.0",
    "bittorrent-tracker": "9.14.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "8.0.0",
    "lint-staged": "9.4.0",
    "memory-chunk-store": "1.3.0",
    "supports-color": "7.0.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.3",
    "webtorrent-fixtures": "1.7.3"
  },
  "scripts": {
    "precommit": "lint-staged --quiet",
    "start": "react-scripts start",
    "start:alt": "PORT=3333 react-scripts start",
    "build": "react-scripts build",
    "build:ci": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "TRACKER_URL=http://localhost:4242/announce react-scripts test",
    "test:ci": "CI=true yarn test",
    "test:coverage": "yarn test --coverage",
    "lint:check": "tslint -c tslint.json -p .",
    "lint:write": "tslint -c tslint.json -p . --fix"
  },
  "lint-staged": {
    "{src,tests}/**/*.tsx?": "yarn lint:check"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
