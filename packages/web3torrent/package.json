    "@storybook/addon-notes": "5.2.5",
    "@storybook/addons": "5.3.9",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.3.9",
    "@types/bencode": "2.0.0",
    "@types/debug": "4.1.5",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/eslint": "6.1.7",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "25.1.0",
    "@types/jszip": "3.1.6",
    "@types/lodash": "4.14.149",
    "@types/node-sass": "4.11.0",
    "@types/pino": "6.0.0",
    "@types/prettier": "1.19.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/supports-color": "5.3.0",
    "@types/webtorrent": "0.107.0",
    "bittorrent-tracker": "9.14.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "ganache-cli": "6.8.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.1.0",
    "jest-junit": "9.0.0",
    "lint-staged": "10.0.4",
    "memory-chunk-store": "1.3.0",
    "pino-pretty": "4.0.0",
    "prettier": "1.19.1",
    "react-docgen-typescript-loader": "3.6.0",
    "supports-color": "7.0.0",
    "ts-jest": "25.0.0",
    "typescript": "3.7.5",
    "webtorrent-fixtures": "1.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "state-channels",
    "webtorrent",
    "webtorrent extension"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts*": "yarn run eslint:check",
    "src/**/*.{scss,json}": "yarn run prettier:check"
  },
  "main": "src/index.js",
  "resolutions": {
    "webtorrent/end-of-stream": "1.4.1"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "build-and-release:tracker": "./tracker/build-push-release.sh",
    "build:ci": "run-s contracts:compile build",
    "build:netlify:production": "yarn build && cp _redirects build/_redirects",
    "build:typescript": "tsc -b .",
    "contracts:compile": "node ./bin/compile.js",
    "eslint:check": "eslint \"**/*.{js,ts,tsx}\" --cache --max-warnings=0",
    "eslint:write": "eslint \"**/*.{js,ts,tsx}\" --fix --max-warnings=0",
    "lint:check": "run-s eslint:check prettier:check",
    "lint:write": "run-s eslint:write prettier:write",
    "precommit": "lint-staged --quiet",
    "prepare": "yarn contracts:compile",
    "prettier:check": "prettier --check '**/*.{scss,json}' --ignore-path '.eslintignore'",
    "prettier:write": "prettier --write '**/*.{scss,json}' --ignore-path '.eslintignore'",
    "release:netlify:production": "netlify deploy --site $WEB3TORRENT_NETLIFY_ID --dir=build",
    "start": "node scripts/start.js",
    "start:alt": "PORT=3333 node scripts/start.js",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "start:tracker": "node tracker/index.js",
    "start:tracker-logs-on": "DEBUG='web3torrent:tracker' yarn start",
    "storybook": "start-storybook -s ./public -p 4568",
    "test": "jest -c ./config/jest/jest.config.js",
    "test:ci": "CI=true yarn test --runInBand --ci --all --forceExit",
    "test:coverage": "yarn test --coverage"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
