{
  "name": "@statechannels/jest-gas-reporter",
  "version": "0.0.2",
  "description": "A jest reporter that reports the gas used by various calls to ethereum contracts.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "repository": "https://github.com/magmo/jest-gas-reporter.git",
  "author": "Alex Gap <alex@magmo.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged --quiet",
    "prepare": "yarn build",
    "build": "tslint --project . && tsc -b",
    "lint:check": "npx tslint --config tslint.json --project .",
    "lint:write": "npx tslint --config tslint.json --project . --fix"
  },
  "lint-staged": {
    "src/*.ts": "yarn lint:check"
  },
  "devDependencies": {
    "@types/easy-table": "0.0.32",
    "@types/jest": "24.0.21",
    "lint-staged": "9.4.2",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0"
  },
  "dependencies": {
    "@statechannels/devtools": "0.1.21",
    "easy-table": "1.1.1",
    "ethers": "4.0.39",
    "path": "0.12.7",
    "solc": "0.5.12",
    "typescript": "3.6.4"
  }
}
