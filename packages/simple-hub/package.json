{
  "name": "@statechannels/simple-hub",
  "description": "nitro hub prototype",
  "version": "0.0.1",
  "author": "",
  "dependencies": {
    "@firebase/app": "0.5.2",
    "@firebase/database": "0.5.19",
    "@sentry/node": "5.13.2",
    "@statechannels/nitro-protocol": "0.0.1",
    "@statechannels/wire-format": "0.0.1",
    "async-lock": "1.2.2",
    "ethers": "4.0.45",
    "firebase": "7.11.0",
    "fp-ts": "2.5.3",
    "pino": "6.2.0",
    "rxfire": "3.11.2",
    "rxjs": "6.5.4"
  },
  "devDependencies": {
    "@statechannels/devtools": "0.1.23",
    "@types/async-lock": "1.1.1",
    "@types/eslint": "6.1.7",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "25.1.0",
    "@types/lodash": "4.14.149",
    "@types/nodemon": "1.19.0",
    "@types/pino": "6.0.0",
    "@types/prettier": "1.19.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "firebase-server": "1.1.0",
    "ganache-cli": "6.8.2",
    "jest": "25.1.0",
    "lint-staged": "10.0.4",
    "nodemon": "1.19.4",
    "npm-run-all": "4.1.5",
    "pino-pretty": "4.0.0",
    "prettier": "1.19.1",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.5"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": "eslint --max-warnings=0"
  },
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "build": "tsc -b .",
    "build:prod": "tsc -p tsconfig.prod.json",
    "firebase-relay:start:sdev": "nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node src/message/firebase-relay",
    "hub:start:dev": "NODE_ENV=development ts-node ./start-dev-server.ts",
    "hub:start:prod": "NODE_ENV=production ts-node ./src/server.ts",
    "hub:test": "jest --runInBand -c ./jest/jest.config.js",
    "hub:watch": "NODE_ENV=development nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./start-dev-server.ts",
    "lint:check": "eslint \"*/**/*.{js,ts,tsx}\" --cache",
    "lint:write": "eslint \"*/**/*.{js,ts,tsx}\" --fix",
    "precommit": "lint-staged --quiet",
    "prepare": "yarn build",
    "prestart": "yarn build && npm run db:rollback && npm run db:migrate && npm run db:seed",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:write": "prettier --write 'src/**/*.{ts,tsx}'",
    "release:hub-w3t-wallet": "./docker/build-push-release.sh",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "test:ci": "yarn hub:test --runInBand"
  }
}
