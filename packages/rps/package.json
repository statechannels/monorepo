    "firebase/**/grpc": "^1.16.1"
  },
  "scripts": {
    "build:ci": "run-s clearContracts build:typescript contracts:compile",
    "build:typescript": "npx tsc -b",
    "build:webpack": "CI=false node scripts/build.js",
    "build": "run-s clearContracts build:typescript build:webpack",
    "clearContracts": "rm -rf ./build/contracts",
    "contracts:compile": "node ./bin/compile.js",
    "lint:check": "tslint -c tslint.json -p .",
    "lint:write": "tslint -c tslint.json -p . --fix",
    "postinstall": "rm -f node_modules/web3/index.d.ts",
    "prettier:check": "npx prettier --check 'src/**/*.{ts,tsx}'",
    "prettier:write": "npx prettier --write 'src/**/*.{ts,tsx}'",
    "start": "yarn contracts:compile && node scripts/start.js",
    "prepare": "yarn build:ci",
    "test:prepare": "yarn clearContracts && yarn contracts:compile",
    "test:app": "npx run-jest -c ./config/jest/jest.config.js",
    "test:contracts": "yarn test:prepare && npx run-jest -c ./config/jest/jest.contracts.config.js",
    "test": "run-s prettier:check test:contracts 'test:app --all'",
    "test:ci": "yarn test:ci:contracts && yarn test:ci:app",
    "test:ci:contracts": "yarn test:contracts --all --ci",
    "test:ci:app": "yarn test:app --all --ci",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "{src,tests}/**/*.tsx?": "yarn lint:check"
  },
