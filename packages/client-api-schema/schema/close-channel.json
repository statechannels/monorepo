{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://statechannels.org/schemas/close-channel.json",
  "definitions": {
    "CloseChannelResult": {
      "type": "object",
      "properties": {
        "participants": {
          "type": "array",
          "items": {
            "$ref": "definitions.json#/definitions/Participant"
          },
          "title": "participants"
        },
        "allocations": {
          "type": "array",
          "items": {
            "$ref": "definitions.json#/definitions/Allocation"
          },
          "title": "allocations"
        },
        "appDefinition": {
          "$ref": "definitions.json#/definitions/Address",
          "title": "appDefinition"
        },
        "appData": {
          "type": "string",
          "title": "appData"
        },
        "status": {
          "type": "string",
          "const": "opening",
          "enum": ["proposed", "opening", "funding", "running", "closing", "closed"]
        },
        "turnNum": {
          "type": "string",
          "const": "0x00"
        }
      },
      "required": ["allocations", "appData", "appDefinition", "participants", "status", "turnNum"]
    },
    "CloseChannelParams": {
      "type": "object",
      "properties": {
        "channelId": {
          "$ref": "definitions.json#/definitions/ChannelId"
        }
      },
      "required": ["channelId"]
    }
  }
}
