{
  "name": "@statechannels/iframe-channel-provider",
  "version": "0.3.14",
  "browser": "dist/iframe-channel-provider.js",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@statechannels/client-api-schema": "0.4.2",
    "eventemitter3": "4.0.7",
    "guid-typescript": "1.0.9",
    "lodash": "4.17.20",
    "normalize-url": "4.5.0",
    "pino": "6.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "7.10.1",
    "@types/debug": "4.1.5",
    "@types/eslint": "7.2.3",
    "@types/eslint-plugin-prettier": "2.2.0",
    "@types/jest": "26.0.14",
    "@types/lodash": "4.14.161",
    "@types/node": "14.11.2",
    "@types/pino": "6.0.0",
    "@types/prettier": "1.19.0",
    "@types/webpack": "4.41.22",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "debug": "4.1.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-tsdoc": "0.2.6",
    "jest": "26.4.2",
    "lint-staged": "10.0.4",
    "pino": "6.2.0",
    "pnp-webpack-plugin": "1.6.4",
    "prettier": "1.19.1",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json",
        "packageJson": "package.json"
      }
    },
    "roots": [
      "./src",
      "./tests"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./src/**/*.ts",
      "!./src/**/index.ts",
      "!./src/**/*.d.ts"
    ]
  },
  "license": "MIT",
  "main": "dist/iframe-channel-provider.js",
  "scripts": {
    "build:typescript": "tsc -b .",
    "generate-api": "api-extractor run --local",
    "lint:check": "eslint \"*/**/*.ts\" --cache",
    "lint:write": "eslint \"*/**/*.ts\" --fix",
    "prepare": "rm -rf dist; yarn build:typescript && yarn webpack && yarn generate-api",
    "prepublish": "yarn webpack",
    "test": "yarn jest --watch",
    "test:ci": "CI=true jest --runInBand --ci --all --detectOpenHandles",
    "test:coverage": "jest --coverage"
  },
  "types": "dist/src/index.d.ts"
}
