{
  "name": "@statechannels/ganache-deployer",
  "version": "0.0.1",
  "description": "Starts ganache and deploys contracts",
  "author": "Alex Gap <alex.gap@consensys.net>",
  "homepage": "https://github.com/statechannels/monorepo#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/*"
  ],
  "bin": {
    "start-ganache-and-deploy": "./lib/start-script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statechannels/monorepo.git"
  },
  "scripts": {
    "start:ganache": "npx ts-node src/start-script.ts",
    "build": "npx tsc",
    "prettier:check": "npx prettier --check 'src/**/*.ts'",
    "prettier:write": "npx prettier --write 'src/**/*.ts'",
    "lint:check": "npx eslint \"*/**/*.{js,ts}\" --cache",
    "lint:write": "npx eslint \"*/**/*.{js,ts}\" --fix",
    "prepare": "yarn run build"
  },
  "bugs": {
    "url": "https://github.com/statechannels/monorepo/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.7.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "1.18.2",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "@statechannels/devtools": "^0.1.21",
    "koa": "2.11.0",
    "ts-node": "8.5.2",
    "write-json-file": "4.2.0"
  }
}
